formulas:
  form: first * 100 + second * 10 + third
properties:
  formula.form:
    displayName: mypropr
views:
  - type: table
    name: Table
    sort:
      - property: tags
        direction: ASC
    filters:
      and:
        - file.folder.contains("many_digits")
        - file.tags.containsAny("even", "odd")
        - file.ext == "md"
        - "!next.isEmpty()"
    order:
      - file.name
      - first
      - second
      - next
      - prev
      - third
      - tags
      - formula.form
  - type: table
    name: Table-even
    sort:
      - property: tags
        direction: ASC
    filters:
      and:
        - file.folder.contains("many_digits")
        - file.tags.contains("even")
        - file.ext == "md"
        - "!next.isEmpty()"
    order:
      - file.name
      - first
      - second
      - next
      - prev
      - third
  - type: cards
    name: cards
    sort:
      - property: tags
        direction: ASC
    filters:
      and:
        - file.folder.contains("many_digits")
        - "!cover.isEmpty()"
    order:
      - file.name
  

